
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

@Html.Partial("/Views/Default/HeadPartial.cshtml")

<body data-spy="scroll" data-target=".navbar" data-offset="40" id="home">

    <!-- Page Navbar -->
    @Html.Partial("/Views/Default/NavbarPartial.cshtml")
    <!-- End of Page Navbar -->
    <!-- page header -->
    @Html.Action("FeaturePartial")
    <!-- end of page header -->
    <!-- about section -->
    @Html.Action("AboutPartial")
    <!-- end of about section -->
    <!-- service section -->
    @Html.Action("ServicePartial")
    <!-- end of service section -->
    <!-- portfolio section -->
    @Html.Action("PortfolioPartial")
    <!-- end of portfolio section -->
    <!-- section -->
    <section class="section-sm bg-primary">
        <!-- container -->
        <div class="container text-center text-sm-left">
            <!-- row -->
            <div class="row align-items-center">
                <div class="col-sm offset-md-1 mb-4 mb-md-0">
                    <h6 class="title text-light">Benimle çalışmak ister misiniz?</h6>
                    <p class="m-0 text-light">Sizlerle çok çılgın projelere adım atacağız</p>
                </div>
                <div class="col-sm offset-sm-2 offset-md-3">
                    <a href="https://www.linkedin.com/in/safak-sahin/" target="_blank">
                        <button class="btn btn-lg my-font btn-light rounded">Beni İşe Al</button>
                    </a>
                </div>
            </div> <!-- end of row -->
        </div> <!-- end of container -->
    </section>
    <!-- end of section -->
    <!-- testimonial section -->
    @Html.Action("TestimonialPartial")
    <!-- end of testimonial section -->
    <!-- contact section -->
    @Html.Action("ContactPartial")
    <!-- end of contact section -->
    <!-- footer -->
    <div class="container">
        <footer class="footer">
            <p class="mb-0">
                Copyright
                <script>document.write(new Date().getFullYear())</script> &copy; 
            </p>
            <div class="social-links text-right m-auto ml-sm-auto">
                <a href="https://www.linkedin.com/in/safak-sahin/" target="_blank" class="link"><i class="ti-linkedin"></i></a>
                <a href="https://github.com/SafakSahinn" target="_blank" class="link"><i class="ti-github"></i></a>
                <a href="#" target="_blank" class="link"><i class="ti-twitter-alt"></i></a>
                <a href="https://www.instagram.com/safak089/" target="_blank" class="link"><i class="ti-instagram"></i></a>
            </div>
        </footer>
    </div>
    <!-- end of page footer -->
    <!-- core  -->
    <script src="~/Templates/meyawo-1.0.0/assets/vendors/jquery/jquery-3.4.1.js"></script>
    <script src="~/Templates/meyawo-1.0.0/assets/vendors/bootstrap/bootstrap.bundle.js"></script>

    <!-- bootstrap 3 affix -->
    <script src="~/Templates/meyawo-1.0.0/assets/vendors/bootstrap/bootstrap.affix.js"></script>

    <!-- Meyawo js -->
    <script src="~/Templates/meyawo-1.0.0/assets/js/meyawo.js"></script>

</body>

</html>
